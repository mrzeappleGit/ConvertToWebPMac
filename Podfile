    inherit! :search_paths
    # Pods for testing
    pod 'SDWebImage'
    pod 'SDWebImageWebPCoder'
  end

  target 'ConvertToWebPUITests' do
    # Pods for testing
    pod 'SDWebImage'
    pod 'SDWebImageWebPCoder'
  end

end
